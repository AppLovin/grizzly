<network-config>
    <transports>
        <transport name="tcp"/>
    </transports>
    <protocols>
        <protocol name="https" security-enabled="true">
            <http/>
            <ssl key-store-type="PKCS12" trust-store-type="JKS" key-store="src/test/resources/s1as.p12"
                 trust-store="src/test/resources/cacerts.jks" ssl3-enabled="false" cert-nickname="1"/>
        </protocol>
    </protocols>
    <network-listeners>
        <thread-pool name="defaultThreadPool"/>
        <network-listener name="https-listener" port="38083" transport="tcp" protocol="https"
                          thread-pool="defaultThreadPool"/>
    </network-listeners>
</network-config>
